<meta http-equiv="Content-Security-Policy" content="default-src 'self'{% if site.environment !== 'production' %} 'unsafe-inline'{% endif %}; child-src 'none'; object-src 'none';">
<meta name="description" content="{{ description or meta.title }}">
<link ref="canonical" href="{{ site.url }}{{ page.url }}">
<link rel="manifest" href="{{ site.url }}/manifest.webmanifest" integrity="{% sri 'manifest.webmanifest' %}">
<link rel="icon" href="{{ site.url }}/favicon.ico" integrity="{% sri 'favicon.ico' %}">
<link rel="icon" href="{{ site.url }}{% rawImage 'src/assets/icons/icon.svg', 'icon.svg' %}" integrity="{% sriImage 'src/assets/icons/icon.svg' %}" type="image/svg+xml">
<link rel="apple-touch-icon" href="{{ site.url }}{% rawImage 'src/assets/icons/apple-touch-icon.png', 'apple-touch-icon' %}" integrity="{% sriImage 'src/assets/icons/apple-touch-icon.png' %}">
{% if metatag and metatag.robots %}
<meta name="robots" content="{{ metatag.robots }}">
{% endif %}
